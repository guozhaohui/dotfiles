[core]
	editor = vim
	autocrlf = false
	safecrlf = true
#[http]
#	proxy = http://proxy.example.com:8080
#[https]
#	proxy = http://proxy.example.com:8080
[push]
	default = simple
[color]
	ui = auto
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	tr = log --oneline --decorate --all --graph
	tr1 = log --decorate --all --graph
	diff1 = diff --no-prefix --relative
[user]
	name = guozhaohui
	email = guo.zhaohui@gmail.com
[merge]
	tool = vimdiff
